{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "pnpm install --frozen-lockfile && pnpm build:web",
  "outputDirectory": "apps/stage-web/dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/:path*\\.(woff|woff2|ttf|otf|eot)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "OPENAI_API_KEY": "",
    "OPENAI_BASE_URL": "https://api.openai.com/v1/",
    "OPENAI_COMPATIBLE_API_KEY": "",
    "OPENAI_COMPATIBLE_BASE_URL": "",
    "OPENROUTER_API_KEY": "",
    "OPENROUTER_BASE_URL": "https://openrouter.ai/api/v1/",
    "ANTHROPIC_API_KEY": "",
    "ANTHROPIC_BASE_URL": "https://api.anthropic.com/v1/",
    "GOOGLE_GENERATIVE_AI_API_KEY": "",
    "GOOGLE_GENERATIVE_AI_BASE_URL": "https://generativelanguage.googleapis.com/v1beta/openai/",
    "DEEPSEEK_API_KEY": "",
    "DEEPSEEK_BASE_URL": "https://api.deepseek.com/",
    "AI302_API_KEY": "",
    "AI302_BASE_URL": "https://api.302.ai/v1/",
    "TOGETHER_API_KEY": "",
    "TOGETHER_BASE_URL": "https://api.together.xyz/v1/",
    "XAI_API_KEY": "",
    "XAI_BASE_URL": "https://api.x.ai/v1/",
    "NOVITA_API_KEY": "",
    "NOVITA_BASE_URL": "https://api.novita.ai/openai/",
    "FIREWORKS_API_KEY": "",
    "FIREWORKS_BASE_URL": "https://api.fireworks.ai/inference/v1/",
    "FEATHERLESS_API_KEY": "",
    "FEATHERLESS_BASE_URL": "https://api.featherless.ai/v1/",
    "PERPLEXITY_API_KEY": "",
    "PERPLEXITY_BASE_URL": "https://api.perplexity.ai/",
    "MISTRAL_API_KEY": "",
    "MISTRAL_BASE_URL": "https://api.mistral.ai/v1/",
    "MOONSHOT_API_KEY": "",
    "MOONSHOT_BASE_URL": "https://api.moonshot.ai/v1/",
    "MODELSCOPE_API_KEY": "",
    "MODELSCOPE_BASE_URL": "https://api-inference.modelscope.cn/v1/",
    "DEFAULT_CHAT_PROVIDER": "openai",
    "DEFAULT_SPEECH_PROVIDER": "openai-audio-speech",
    "DEFAULT_TRANSCRIPTION_PROVIDER": "openai-audio-transcription"
  }
}
